def movezero(arr):
    if len(arr)<1 or len(arr)>1000:
        return out_of_range
    
    c=0
    for i in range (len(arr)):
        if (arr[i]!=0):
            arr[c]=arr[i]
            c+=1
            
    while c<len(arr):
        arr[c]=0
        c+=1
    return arr
    
    
    arr=[0,0,1,2,3,0,1,2,0,0,0]
